<!DOCTYPE html>
<html lang="es">
<head>
  <%- include('src/head') %>
</head>
<body data-senna="data-senna" data-senna-surface="data-senna-surface">
  <div class="senna-loading-bar"></div>
  
  <header>
    <h1><a href="/" title="DrkBot"><span id="header"><%= botName %></span></a></h1>
    <%- include('src/header') %>
  </header>
  
  <div class="container">
    <div class="grid">
      <!-- Contenido -->
      <div class="col-7_sm-12">
         <h2>Planes</h2>
         <div class="pricing">
            <h2 class="package">Gratis</h2>
            <span class="limit">10 Usos / Cuenta</span>
            <span class="price"><b>$ 0</b></span>
            <br>
            <br>
         </div>
         <hr>
         <div class="pricing">
            <h2 class="package">Bronce</h2>
            <span class="limit">Ilimitado / Mes</span>
            <span class="price"><b>$ 1</b></span>
            <br>
            <a href="/orden?plan=bronce"><button> Ordenar ➠ </button></a>
         </div>
         <hr>
         <div class="pricing">
            <h2 class="package">Plata</h2>
            <span class="limit">Ilimitado / 6 Meses</span>
            <span class="price"><b>$ 5.5</b></span>
            <br>
            <a href="/orden?plan=plata"><button> Ordenar ➠ </button></a>
         </div>
         <hr>
         <div class="pricing">
            <h2 class="package">Oro</h2>
            <span class="limit">Ilimitado / 1 Año</span>
            <span class="price"><b>$ 10</b></span>
            <br>
            <a href="/orden?plan=oro"><button> Ordenar ➠ </button></a>
         </div>
      </div>
      
      <!-- Informacion General -->
      <div class="col-5_sm-12">
        <%- include('src/end') %>
      </div>
    </div>
  </div>

  <footer>© DrkBot</footer>
  <script src="/assets/js/main.js" type="text/javascript"></script>
</body>
</html>